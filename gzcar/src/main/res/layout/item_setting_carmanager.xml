<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="40dp"
    android:background="#fff"
    android:orientation="horizontal">


    <TextView
        android:id="@+id/item_setting_carmanager_id"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:background="@mipmap/ic_text_bg"
        android:gravity="center"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/item_setting_carmanager_carnumber"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:background="@mipmap/ic_text_bg"
        android:gravity="center"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/item_setting_carmanager_type"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:background="@mipmap/ic_text_bg"
        android:gravity="center"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/item_setting_carmanager_carwei"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:background="@mipmap/ic_text_bg"
        android:gravity="center"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/item_setting_carmanager_person"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:background="@mipmap/ic_text_bg"
        android:gravity="center"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/item_setting_carmanager_phone"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:background="@mipmap/ic_text_bg"
        android:gravity="center"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/item_setting_carmanager_address"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:background="@mipmap/ic_text_bg"
        android:gravity="center"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/item_setting_carmanager_startdate"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="2"
        android:background="@mipmap/ic_text_bg"
        android:gravity="center"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/item_setting_carmanager_enddate"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="2"
        android:background="@mipmap/ic_text_bg"
        android:gravity="center"
        android:textSize="18sp" />
</LinearLayout>